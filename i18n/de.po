#. module: ayu_back_in_stock
#: model:mail.template,subject:ayu_back_in_stock.mail_template_back_in_stock
msgid "${object.product_name} is Back in Stock!"
msgstr "${object.product_name} ist wieder verfügbar!"

#. module: ayu_back_in_stock
#: model_terms:ir.ui.view,arch_db:ayu_back_in_stock.view_notification_form
msgid "(Re)send Registration Mail"
msgstr "Registrierungs-Mail (erneut) Senden"

#. module: ayu_back_in_stock
#: model:mail.template,body_html:ayu_back_in_stock.mail_template_back_in_stock
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"  <p style=\"margin: 0px; padding: 0px; font-size: 12px;\">\n"
"    Hello,\n"
"    <br/><br/>\n"
"    <p>\n"
"      <a href=\"${object.product_id.website_url | safe}\">${object.product_name}</a>\n"
"      is available again!\n"
"    </p>\n"
"    <p>React quick, so you don't miss it.</p>\n"
"  </p>\n"
"</div>"
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"  <p style=\"margin: 0px; padding: 0px; font-size: 12px;\">\n"
"    Hello,\n"
"    <br/><br/>\n"
"    <p>\n"
"      <a href=\"${object.product_id.website_url | safe}\">${object.product_name}</a>\n"
"      ist wieder verfügbar!\n"
"    </p>\n"
"    <p>Handeln Sie schnell, so dass Sie es nicht verpassen.</p>\n"
"  </p>\n"
"</div>"

#. module: ayu_back_in_stock
#: model:mail.template,body_html:ayu_back_in_stock.mail_template_registration
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"  <p style=\"margin: 0px; padding: 0px; font-size: 12px;\">\n"
"    Hello,\n"
"    <br/><br/>\n"
"    <p>Thank you for the interest in our product\n"
"      <a href=\"${object.product_id.website_url | safe}\">${object.product_name}</a>!\n"
"    </p>\n"
"    <p>We received your registration and will notify you as soon as it's back in stock. Please apologize the waiting time.</p>\n"
"    <p>Do not hesitate to contact us if you have any questions.</p>\n"
"  </p>\n"
"</div>"
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"  <p style=\"margin: 0px; padding: 0px; font-size: 12px;\">\n"
"    Hallo,\n"
"    <br/><br/>\n"
"    <p>Vielen Dank für Ihr Interesse in unser Produkt\n"
"      <a href=\"${object.product_id.website_url | safe}\">${object.product_name}</a>!\n"
"    </p>\n"
"    <p>Wir haben Ihre Registrierung erhalten und werden Sie benachrichtigen, sobald es wieder auf Lager ist. Bitte entschuldigen Sie die Wartezeit.</p>\n"
"    <p>Haben Sie Fragen? Bitte nicht zögern und kontaktieren Sie uns.</p>\n"
"  </p>\n"
"</div>"

